{
  "name": "fum",
  "version": "0.3.2",
  "private": true,
  "type": "module",
  "scripts": {
    "script": "tsx",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "hardhat test",
    "coverage": "hardhat coverage",
    "dev:link": "npm link fum_library",
    "dev:unlink": "npm unlink fum_library && npm install file:../fum_library",
    "test-contracts": "bash -c './sync-contracts.sh && cd ~/code/fum_testing && npx hardhat test'",
    "test-contracts:coverage": "bash -c './sync-contracts.sh && cd ~/code/fum_testing && npx hardhat coverage'",
    "hardhat-node": "bash -c 'cd ~/code/fum_testing && npx hardhat node'",
    "contracts:sync": "tsx scripts/sync-contracts-to-ecosystem.js",
    "deployment-setup": "npm run contracts:sync",
    "seed-hardhat-localhost": "bash -c 'npm run script scripts/deploy.js --network=localhost && npm run script scripts/create-test-vault.js --network=localhost && npm run script scripts/seed.js --network=localhost'",
    "ganache": "node scripts/start-ganache.js",
    "seed-localhost": "bash -c 'npm run script scripts/create-test-vault.js --network=localhost && npm run script scripts/seed.js --network=localhost'",
    "test-automation": "node scripts/test-automation.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.0",
    "@uniswap/v3-sdk": "^3.24.1",
    "@web3modal/ethereum": "^2.7.1",
    "bootstrap": "^5.3.3",
    "ethers": "^6.13.5",
    "fum_library": "file:../fum_library",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-redux": "^9.2.0",
    "solc": "^0.8.29"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@openzeppelin/contracts": "^5.2.0",
    "@pancakeswap/v3-core": "^1.0.2",
    "@pancakeswap/v3-periphery": "^1.0.2",
    "@typechain/ethers-v6": "^0.4.0",
    "@typechain/hardhat": "^8.0.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "chai": "^5.2.0",
    "dotenv": "^16.4.7",
    "ganache": "^7.9.2",
    "hardhat": "^2.22.19",
    "hardhat-gas-reporter": "^1.0.8",
    "lucide-react": "^0.487.0",
    "react-bootstrap-icons": "^1.11.5",
    "solidity-coverage": "^0.8.2",
    "tsx": "^4.19.3",
    "typechain": "^8.2.0"
  }
}
